{
  "name": "web",
  "version": "0.0.1",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "reason": { "react-jsx": 3 },
  "bs-dependencies": ["@rescript/react"],
  "warnings": {
    "error": "+101"
  },
  "gentypeconfig": {
    "language": "typescript",
    "debug": {
      "all": false,
      "basic": false
    },
    "importPath": "relative",
    "shims": {
      "Js": "Js",
      "React": "ReactShim",
      "ReactEvent": "ReactEvent",
      "ReasonPervasives": "ReasonPervasives",
      "ReasonReact": "ReactShim"
    },
    "exportInterfaces": false
  }
}
